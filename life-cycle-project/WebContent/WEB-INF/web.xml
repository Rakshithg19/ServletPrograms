<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" id="WebApp_ID" version="4.0">
  <display-name>life-cycle-project</display-name>
  <welcome-file-list>
    <welcome-file>lifeCycle.html</welcome-file>
  </welcome-file-list>
  <servlet>
   <servlet-name>life</servlet-name>
  <servlet-class>org.dev.devapp.LifeCycleServlet</servlet-class>
    <init-param>
  <param-name>name</param-name>
  <param-value>BeginningOfLifeCycle</param-value>
  </init-param>
  <init-param>
  <param-name>age</param-name>
  <param-value>56</param-value>
  </init-param>
  <load-on-startup>+99</load-on-startup>
  </servlet>

  <servlet-mapping>
  <servlet-name>life</servlet-name>
  <url-pattern>/lifeCycle</url-pattern>
  </servlet-mapping>
</web-app>